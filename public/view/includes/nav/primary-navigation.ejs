            <script  src="assets/js/primary-navigation.js"></script>
            <script >
                const mobileMenuButton = document.querySelector("button.mobile-menu-button");
                const mobileMenu = document.querySelector(".mobile-menu");
                const primaryNav = document.querySelector(".primaryNav");
                const rightSideWithSession = document.getElementById('right_side_with_session');
                const rightSideWithNoSession = document.getElementById('right_side_with_no_session');
                const sessionUuid = '<%= data.uuid %>';

                if (sessionUuid) {
                    rightSideWithSession.style.display = 'block';
                    rightSideWithNoSession.style.display = 'none';
                } else {
                    rightSideWithSession.style.display = 'none';
                    rightSideWithNoSession.style.display = 'block';
                }

                function toggleNav() {
                    const w = window.innerWidth;
                    if (w < 1015) {
                        mobileMenu.classList.remove("hidden");
                        primaryNav.classList.add("hidden");
                    }
                    if (w > 1015) {
                        mobileMenu.classList.add("hidden");
                        primaryNav.classList.remove("hidden");
                    }

                    if (w > 1280) {
                        // primaryNav.style.marginLeft = "-150px";
                    } else {
                        primaryNav.style.marginLeft = null;
                    }

                    if (w > 1460) {
                        // primaryNav.style.marginLeft = "-150px";
                    } else {
                        primaryNav.style.marginLeft = null;
                    }
                }

                window.addEventListener('resize', () => {
                    toggleNav();
                });

                window.addEventListener('load', () => {
                    toggleNav();
                });

                function goToHomePage() {
                    location.replace(host + '/');
                }

                function goToSelectionPage() {
                    location.replace(host + '/selection');
                }

                function goToLoginPage() {
                    location.replace(host + '/login');
                }
            </script>

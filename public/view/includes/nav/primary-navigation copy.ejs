        </div>
        <!-- mobile menu -->
        <div class="hidden mobile-menu">
            <div class="grid grid-cols-7">
                <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-4"
                    uk-tooltip="title: Home; pos: bottom">
                    <a class="" href="#" onclick="goToHomePage()">
                        <span class="icon-feather-home nav-span" > </span>
                    </a>
                </div>
                <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-4"
                    uk-tooltip="title: Event Calendar; pos: bottom">
                    <a class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100"
                        href="/#eventCalendar">
                        <span class="icon-feather-calendar nav-span" > </span>
                    </a>
                </div>
                <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-4"
                    uk-tooltip="title: Traders Videos; pos: bottom">
                    <a class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100"
                        href="/#tradersVideos">
                        <span class="icon-feather-video nav-span" > </span>
                    </a>
                </div>
                <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-4"
                    uk-tooltip="title: Traders Webinars; pos: bottom">
                    <a class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100"
                        href="/#tradersWebinars">
                        <span class="icon-feather-info nav-span" > </span>
                    </a>
                </div>
                <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-4"
                    uk-tooltip="title: Pricing; pos: bottom">
                    <a class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100"
                        href="/pricing">
                        <span class="icon-feather-tag nav-span" > </span>
                    </a>
                </div>
                <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-4"
                    uk-tooltip="title: Selections; pos: bottom">
                    <a href="#" onclick="goToSelectionPage()"
                        class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100">
                        <span class="icon-feather-users nav-span" > </span>
                    </a>
                </div>
                <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-4"
                    uk-tooltip="title: Help / Suggestion; pos: bottom">
                    <a href="#" onclick="goToHelpSuggestionPage()"
                        class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100">
                        <span class="icon-feather-message-square nav-span" > </span>
                    </a>
                </div>
            </div>
    
            <script  src="assets/js/primary-navigation.js"></script>
            <script >
                const btn = document.querySelector("button.mobile-menu-button");
                const mobileMenu = document.querySelector(".mobile-menu");
         
                // btn.addEventListener("click", () => {
                //   mobileMenu.classList.toggle("hidden");
                // });
    
                let w = window.innerWidth;
    
                if (w < 768) {
                    mobileMenu.classList.remove("hidden");
                }
                if (w > 768) {
                    mobileMenu.classList.add("hidden");
                }
    
                addEventListener('resize', (event) => {
                    let w = window.innerWidth;
    
                    if (w < 768) {
                        mobileMenu.classList.remove("hidden");
                    }
                    if (w > 768) {
                        mobileMenu.classList.add("hidden");
                    }
                });

                let sessionUuid = `<%= data.uuid %>`;
                
                if (sessionUuid) {
                    document.getElementById('right_side_with_session').style.display = 'block';
                    document.getElementById('right_side_with_no_session').style.display = 'none';
                } else {
                    document.getElementById('right_side_with_session').style.display = 'none';
                    document.getElementById('right_side_with_no_session').style.display = 'block';
                }
    
                function goToHomePage() {
                    location.replace(host + '/');
                }
    
    
                function goToSelectionPage() {
                    location.replace(host + '/selection');
                }
    
                

                addEventListener("load", (event) => {
                    let innerWidth = window.innerWidth;

                    if (innerWidth > 1280) {
                        document.getElementById("primaryNav").style.marginLeft = "-250px";
                    } else {
                        document.getElementById("primaryNav").style.marginLeft = null;
                    }

                    if (innerWidth > 1460) {
                        document.getElementById("primaryNav").style.marginLeft = "-250px";
                    } else {
                        document.getElementById("primaryNav").style.marginLeft = null;
                    }

                });

                addEventListener('resize', (event) => {
                    let innerWidth = window.innerWidth;

                    if (innerWidth > 1280) {
                        document.getElementById("primaryNav").style.marginLeft = "-250px";
                    } else {
                        document.getElementById("primaryNav").style.marginLeft = null;
                    }

                    if (innerWidth > 1460) {
                        document.getElementById("primaryNav").style.marginLeft = "-250px";
                    } else {
                        document.getElementById("primaryNav").style.marginLeft = null;
                    }
                });

                function goToLoginPage() {
                    location.replace(host + '/login');
                }
            </script>
            <!-- </ul> -->
        </div>
    
    </nav>
</header>
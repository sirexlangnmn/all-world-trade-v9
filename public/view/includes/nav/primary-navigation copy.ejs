                            uk-tooltip="title: Traders Webinars; pos: bottom">
                            <a class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100"
                                href="/#tradersWebinars">
                                <span class="icon-feather-info nav-span" > </span>
                            </a>
                        </div>
                        <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-10"
                            uk-tooltip="title: Pricing; pos: bottom">
                            <a class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100"
                                href="/pricing">
                                <span class="icon-feather-tag nav-span" > </span>
                            </a>
                        </div>
                        <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-10"
                            uk-tooltip="title: Selections; pos: bottom">
                            <a href="#" onclick="goToSelectionPage()"
                                class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100">
                                <span class="icon-feather-users nav-span" >
                                </span>
                            </a>
                        </div>
                        <div class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100 py-3 px-10"
                            uk-tooltip="title: Help / Suggestion; pos: bottom">
                            <a href="#" onclick="goToHelpSuggestionPage()"
                                class="text-center block border-white rounded-custom hover:border-gray-100 hover:bg-gray-100">
                                <span class="icon-feather-message-square nav-span" >
                                </span>
                            </a>
                        </div>
                    </div>
                </div>
    
                <!-- Secondary Navbar items -->
                <div class="md:flex items-center py-2 px-2 ">
                    <div class="right_side" id="right_side_with_session">
                        <div class="header_widgets" id="primaryWidgets">
                            <!-- === CART - START === -->
      
                            <!-- === CART - END === -->
                        
                            <!-- === NOTIFICATIONS - START === -->
               

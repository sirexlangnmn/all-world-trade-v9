<%- include('../includes/head/index.ejs') %>
<body>
    <div id="wrapper" class="is-collapse is-active">
        <!-- === HEADER - START === -->
        <%- include('../includes/header-for-registration-page/index.ejs') %>
        <!-- === HEADER - END === -->
        <!-- Main Contents -->
        <div class="main_content_custom mt-8">
            <!-- === REGISTRATION NAV - START === -->
            <%- include('../includes/registration/registration_nav.ejs') %>
            <!-- === REGISTRATION NAV - END === -->
            <div class="mcontainer">
                <form action="javascript:void(0);" method="post" id="testForm" name="testForm" enctype="multipart/form-data" accept-charset="utf-8">
                    <!-- <div class="profile is_page"> -->
                    <div class="profile">
                        <div class="profiles_banner profile_avatar image-upload">
                            <label for="testBanner">
                            <img id="testBannerPreview" src="uploads/placeholder/banner-placeholder2.jpg" />
                            </label>
                            <input type="file" id="testBanner" name="testBanner" />
                        </div>
                        <p class="text-red-500 orbitron" style="text-align: center;" id="testBannerValidation"> This image is strictly required in order to join the selection interface </p>
                        <div class="profiles_content">
                            <div class="profile_avatar image-upload">
                                <label for="testLogo">
                                <img id="testLogoPreview" src="uploads/placeholder/logo-placeholder2.jpg" />
                                </label>
                                <input type="file" id="testLogo" name="testLogo" />
                                <p class="text-red-500" style="text-align: center; margin-top: -18px" id="testLogoValidation"></p>
                            </div>
                        </div>
                    </div>
                    <div>
                        <label for="" class="orbitron"> Test Name: </label>
                        <input type="text" placeholder="" class="shadow-none with-border " id="testName" name="testName">
                        <p class="text-red-500" id="testNameValidation"></p>
                    </div>
                    <div class="text-right p-4">
                        <button type="button" class="button bg-blue-700" id="btnTestForm"> Save </button>
                      </div>
                </form>
            </div>
        </div>
    </div>
    <%- include('../includes/scripts/index.ejs') %>
    <script  src="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/11.3.0/sweetalert2.min.js"></script>
    <script  src="assets/js/file-and-input-test.js"></script>
</body>
</html>